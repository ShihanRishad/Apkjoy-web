{
    "trailingSlash": true,
    "rewrites": [
      { "source": "/about/", "destination": "/" },
      { "source": "/download/", "destination": "/" },
      { "source": "/help/", "destination": "/" },
      {
        "source": "/(.*)",
        "has": [
          { "type": "file" }
        ],
        "destination": "/$1"
      },
      { "source": "/(.*)", "destination": "/" }
    ]
  }
  